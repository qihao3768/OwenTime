<?xml version="1.0" encoding="utf-8"?>
<!-- 标签的背景 label_bg -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 标签选中时的背景 -->
    <item android:state_selected="true">
        <shape>
            <stroke android:width="1dp" android:color="#FFECD6" />
            <corners android:radius="6dp" />
            <solid android:color="@color/FFECD6" />
        </shape>
    </item>
    <!-- 标签的正常背景 -->
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/F5F5F5" />
            <corners android:radius="6dp" />
            <solid android:color="@color/F5F5F5" />
        </shape>
    </item>
</selector>